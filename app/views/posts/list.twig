{% extends "abstracts/base.twig" %}

{% block title %}
    users
{% endblock %}

{% block content %}
    <h1>posts:</h1>
    <hr>
    <ul>
        {% for post in posts %}
            <li>
                <a href="/posts/{{ post.id }}">{{ post.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}